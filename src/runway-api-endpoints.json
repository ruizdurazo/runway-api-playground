{
  "endpoints": [
    {
      "name": "Generate a video from an image",
      "method": "POST",
      "path": "/v1/image_to_video",
      "description": "This endpoint will start a new task to generate a video from an image prompt.",
      "headers": {
        "X-Runway-Version": {
          "required": true,
          "value": "2024-11-06"
        }
      },
      "inputs": {
        "promptImage": {
          "type": "string or Array of PromptImages (objects)",
          "required": true,
          "description": "One of the following shapes: string <uri> A HTTPS URL or data URI containing an encoded image to be used as the first frame of the generated video. Array of objects with uri and position."
        },
        "model": {
          "type": "string",
          "required": true,
          "validValues": ["gen4_turbo", "gen3a_turbo", "veo3"]
        },
        "ratio": {
          "type": "string",
          "required": true,
          "default": "1280:768",
          "validValues": [
            "1280:720",
            "720:1280",
            "1104:832",
            "832:1104",
            "960:960",
            "1584:672",
            "1280:768",
            "768:1280"
          ]
        },
        "seed": {
          "type": "integer",
          "constraints": "[0 .. 4294967295]"
        },
        "promptText": {
          "type": "string",
          "constraints": "<= 1000 characters"
        },
        "duration": {
          "type": "integer",
          "default": 10,
          "validValues": [5, 8, 10]
        },
        "contentModeration": {
          "type": "object",
          "subfields": {
            "publicFigureThreshold": {
              "type": "string",
              "default": "auto",
              "validValues": ["auto", "low"]
            }
          }
        }
      },
      "outputs": {
        "id": "string"
      }
    },
    {
      "name": "Generate a video from text",
      "method": "POST",
      "path": "/v1/text_to_video",
      "description": "This endpoint will start a new task to generate a video from a text prompt.",
      "headers": {
        "X-Runway-Version": {
          "required": true,
          "value": "2024-11-06"
        }
      },
      "inputs": {
        "promptText": {
          "type": "string",
          "required": true,
          "constraints": "<= 1000 characters"
        },
        "model": {
          "type": "string",
          "required": true,
          "validValues": ["veo3"]
        },
        "ratio": {
          "type": "string",
          "required": true,
          "validValues": ["1280:720", "720:1280"]
        },
        "duration": {
          "type": "integer",
          "required": true,
          "value": 8
        },
        "seed": {
          "type": "integer",
          "constraints": "[0 .. 4294967295]"
        }
      },
      "outputs": {
        "id": "string"
      }
    },
    {
      "name": "Generate a video from a video",
      "method": "POST",
      "path": "/v1/video_to_video",
      "description": "This endpoint will start a new task to generate a video from a video.",
      "headers": {
        "X-Runway-Version": {
          "required": true,
          "value": "2024-11-06"
        }
      },
      "inputs": {
        "videoUri": {
          "type": "string <uri>",
          "required": true
        },
        "promptText": {
          "type": "string",
          "required": true,
          "constraints": "<= 1000 characters"
        },
        "model": {
          "type": "string",
          "required": true,
          "validValues": ["gen4_aleph"]
        },
        "ratio": {
          "type": "string",
          "required": true,
          "validValues": [
            "1280:720",
            "720:1280",
            "1104:832",
            "960:960",
            "832:1104",
            "1584:672",
            "848:480",
            "640:480"
          ]
        },
        "seed": {
          "type": "integer",
          "constraints": "[0 .. 4294967295]"
        },
        "references": {
          "type": "Array of objects",
          "subfields": {
            "type": {
              "type": "string",
              "validValues": ["image"]
            },
            "uri": {
              "type": "string <uri>",
              "required": true
            }
          }
        },
        "contentModeration": {
          "type": "object",
          "subfields": {
            "publicFigureThreshold": {
              "type": "string",
              "default": "auto",
              "validValues": ["auto", "low"]
            }
          }
        }
      },
      "outputs": {
        "id": "string"
      }
    },
    {
      "name": "Generate an image from text",
      "method": "POST",
      "path": "/v1/text_to_image",
      "description": "This endpoint will start a new task to generate images from text.",
      "headers": {
        "X-Runway-Version": {
          "required": true,
          "value": "2024-11-06"
        }
      },
      "inputs": {
        "promptText": {
          "type": "string",
          "required": true,
          "constraints": "<= 1000 characters"
        },
        "ratio": {
          "type": "string",
          "required": true,
          "validValues": [
            "1920:1080",
            "1080:1920",
            "1024:1024",
            "1360:768",
            "1080:1080",
            "1168:880",
            "1440:1080",
            "1080:1440",
            "1808:768",
            "2112:912",
            "1280:720",
            "720:1280",
            "720:720",
            "960:720",
            "720:960",
            "1680:720",
            "1344:768",
            "768:1344",
            "1024:1024",
            "1184:864",
            "864:1184",
            "1536:672"
          ]
        },
        "model": {
          "type": "string",
          "required": true,
          "validValues": ["gen4_image_turbo", "gen4_image", "gemini_2.5_flash"]
        },
        "seed": {
          "type": "integer",
          "constraints": "[0 .. 4294967295]"
        },
        "referenceImages": {
          "type": "Array of objects",
          "description": "An array of up to three images to be used as references for the generated image output. For gen4_image_turbo, at least one reference image is required.",
          "subfields": {
            "uri": {
              "type": "string <uri>",
              "required": true
            },
            "tag": {
              "type": "string",
              "constraints": "A name used to refer to the image reference, from 3 to 16 characters in length."
            }
          }
        },
        "contentModeration": {
          "type": "object",
          "subfields": {
            "publicFigureThreshold": {
              "type": "string",
              "default": "auto",
              "validValues": ["auto", "low"]
            }
          }
        }
      },
      "outputs": {
        "id": "string"
      }
    },
    {
      "name": "Upscale a video",
      "method": "POST",
      "path": "/v1/video_upscale",
      "description": "This endpoint will start a new task to upscale a video. Videos will be upscaled by a factor of 4X, capped at a maximum of 4096px along each side.",
      "headers": {
        "X-Runway-Version": {
          "required": true,
          "value": "2024-11-06"
        }
      },
      "inputs": {
        "videoUri": {
          "type": "string <uri>",
          "required": true,
          "description": "The video must be less than 4096px on each side. The video duration may not exceed 40 seconds."
        },
        "model": {
          "type": "string",
          "required": true,
          "validValues": ["upscale_v1"]
        }
      },
      "outputs": {
        "id": "string"
      }
    },
    {
      "name": "Control a character",
      "method": "POST",
      "path": "/v1/character_performance",
      "description": "This endpoint will start a new task to control a character's facial expressions and body movements using a reference video.",
      "headers": {
        "X-Runway-Version": {
          "required": true,
          "value": "2024-11-06"
        }
      },
      "inputs": {
        "character": {
          "type": "object",
          "required": true,
          "description": "The character to control. You can either provide a video or an image.",
          "subfields": {
            "type": {
              "type": "string",
              "required": true,
              "validValues": ["video", "image"]
            },
            "uri": {
              "type": "string <uri>",
              "required": true
            }
          }
        },
        "reference": {
          "type": "object",
          "required": true,
          "subfields": {
            "type": {
              "type": "string",
              "required": true,
              "validValues": ["video"]
            },
            "uri": {
              "type": "string <uri>",
              "required": true,
              "description": "The video must be between 3 and 30 seconds in duration."
            }
          }
        },
        "model": {
          "type": "string",
          "required": true,
          "validValues": ["act_two"]
        },
        "ratio": {
          "type": "string",
          "required": true,
          "validValues": [
            "1280:720",
            "720:1280",
            "960:960",
            "1104:832",
            "832:1104",
            "1584:672"
          ]
        },
        "bodyControl": {
          "type": "boolean",
          "default": true
        },
        "expressionIntensity": {
          "type": "integer",
          "default": 3,
          "constraints": "[1 .. 5]"
        },
        "seed": {
          "type": "integer",
          "constraints": "[0 .. 4294967295]"
        },
        "contentModeration": {
          "type": "object",
          "subfields": {
            "publicFigureThreshold": {
              "type": "string",
              "default": "auto",
              "validValues": ["auto", "low"]
            }
          }
        }
      },
      "outputs": {
        "id": "string"
      }
    },
    {
      "name": "Get task detail",
      "method": "GET",
      "path": "/v1/tasks/{id}",
      "description": "Return details about a task. Consumers of this API should not expect updates more frequent than once every five seconds for a given task.",
      "headers": {
        "X-Runway-Version": {
          "required": true,
          "value": "2024-11-06"
        }
      },
      "inputs": {
        "id": {
          "type": "string <uuid>",
          "required": true,
          "description": "The ID of a previously-submitted task that has not been canceled or deleted."
        }
      },
      "outputs": {
        "id": "string",
        "status": {
          "type": "string",
          "validValues": [
            "RUNNING",
            "SUCCEEDED",
            "FAILED",
            "PENDING",
            "CANCELLED",
            "THROTTLED"
          ]
        },
        "createdAt": "string <date-time>",
        "failure": "string",
        "failureCode": "string",
        "output": "array of strings <uri>",
        "progress": "number"
      }
    },
    {
      "name": "Cancel or delete a task",
      "method": "DELETE",
      "path": "/v1/tasks/{id}",
      "description": "Tasks that are running, pending, or throttled can be canceled by invoking this method. Invoking this method for other tasks will delete them.",
      "headers": {
        "X-Runway-Version": {
          "required": true,
          "value": "2024-11-06"
        }
      },
      "inputs": {
        "id": {
          "type": "string <uuid>",
          "required": true,
          "description": "The ID of a previously-submitted task that has not been canceled or deleted."
        }
      },
      "outputs": {
        "description": "204 No Content on success."
      }
    },
    {
      "name": "Get organization information",
      "method": "GET",
      "path": "/v1/organization",
      "description": "Get usage tier and credit balance information about the organization associated with the API key used to make the request.",
      "headers": {
        "X-Runway-Version": {
          "required": true,
          "value": "2024-11-06"
        }
      },
      "inputs": {},
      "outputs": {
        "creditBalance": "integer",
        "tier": {
          "maxMonthlyCreditSpend": "integer",
          "models": "object with model limits"
        },
        "usage": {
          "models": "object with model usage"
        }
      }
    },
    {
      "name": "Query credit usage",
      "method": "POST",
      "path": "/v1/organization/usage",
      "description": "Fetch credit usage data broken down by model and day for the organization associated with the API key used to make the request. Up to 90 days of data can be queried at a time.",
      "headers": {
        "X-Runway-Version": {
          "required": true,
          "value": "2024-11-06"
        }
      },
      "inputs": {
        "startDate": {
          "type": "string <date>",
          "description": "The start date of the usage data in ISO-8601 format (YYYY-MM-DD). If unspecified, it will default to 30 days before the current date."
        },
        "beforeDate": {
          "type": "string <date>",
          "description": "The end date of the usage data in ISO-8601 format (YYYY-MM-DD), not inclusive. If unspecified, it will default to thirty days after the start date."
        }
      },
      "outputs": {
        "results": "array of objects with date and usedCredits",
        "models": "array of strings"
      }
    }
  ]
}
